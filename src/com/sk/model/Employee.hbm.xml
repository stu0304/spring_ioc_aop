<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.sk.model.Employee"  table="t_employee">
<id name="eid" type="java.lang.String">
<column name="t_eid"></column>
   <generator class="assigned"></generator>
</id>
<property name="ename" type="java.lang.String" column="t_ename"></property>
<property name="deptid" type="java.lang.String" column="t_deptid"></property>

</class>
</hibernate-mapping>
